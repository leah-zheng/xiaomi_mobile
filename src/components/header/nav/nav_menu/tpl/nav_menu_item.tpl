<li class="phone-card">
    <a href="./detail.html?id={{id}}" target="_blank">
        <div class="thumb {{isFrist}}">
            <img src="{{pic}}" alt="{{phone_name}}">
        </div>
        <div class="info">
            <h3>{{phone_name}}</h3>
            <p>{{default_price}}</p>
        </div>
        
    </a>
</li>